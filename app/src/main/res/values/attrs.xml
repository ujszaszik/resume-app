<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvatarImageBehavior">
        <attr name="finalYPosition" format="dimension" />
        <attr name="startXPosition" format="dimension" />
        <attr name="startToolbarPosition" format="dimension" />
        <attr name="startHeight" format="dimension" />
        <attr name="finalHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SkillRow">
        <attr name="skillName" format="string" />
        <attr name="skillDescription" format="string" />
        <attr name="skillStars" format="float" />
        <attr name="skillImage" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExperienceRow">
        <attr name="experienceName" format="string" />
        <attr name="experienceTitle" format="string" />
        <attr name="experienceDate" format="string" />
        <attr name="experienceDescription" format="string" />
        <attr name="experienceRoles" format="string" />
        <attr name="experienceTechnologies" format="string" />
    </declare-styleable>

    <declare-styleable name="LanguageRow">
        <attr name="languageFlag" format="integer" />
        <attr name="languageName" format="string" />
        <attr name="languageDescription" format="string" />
        <attr name="languageRating" format="float" />
    </declare-styleable>

    <declare-styleable name="PersonalityRow">
        <attr name="personalityImage" format="integer" />
        <attr name="personalityName" format="string" />
        <attr name="personalityDescription" format="string" />
    </declare-styleable>

    <declare-styleable name="QuoteRow">
        <attr name="quoteImage" format="integer" />
        <attr name="quoteDescription" format="string" />
        <attr name="quoteFrom" format="string" />
        <attr name="quoteFromTitle" format="string" />
    </declare-styleable>
</resources>